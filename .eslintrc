{
    "parser": "@typescript-eslint/parser",
    "parserOptions": {
        "ecmaVersion": 12,
        "sourceType": "module"
    },
    "plugins": ["@typescript-eslint"],
    "extends": [
        "eslint:recommended",
        "plugin:@typescript-eslint/recommended",
        "prettier"
    ],
    "rules": {
        "valid-typeof": "error",
        "@typescript-eslint/consistent-type-definitions": "off",
        "@typescript-eslint/no-non-null-assertion": "off"
    },
    "env": {
        "browser": true,
        "es2021": true
    }
}
